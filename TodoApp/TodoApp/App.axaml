<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:icons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             x:Class="TodoApp.App"
			 RequestedThemeVariant="Light">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

	<Application.Resources>
		<SolidColorBrush x:Key="Warning">#ff9966</SolidColorBrush>
		<SolidColorBrush x:Key="Success">#198754</SolidColorBrush>
	</Application.Resources>
	
    <Application.Styles>
        <icons:MaterialIconStyles />
		<StyleInclude Source="/Styles/TodoStyles.axaml" />
		<StyleInclude Source="/Views/IconText.axaml" />
		<StyleInclude Source="/Views/IconToggleButton.axaml" />
        <FluentTheme/>
    </Application.Styles>
</Application>
